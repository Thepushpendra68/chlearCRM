{
  "version": 2,
  "outputDirectory": "frontend/dist",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60,
      "memory": 1024,
      "runtime": "nodejs24.x"
    }
  },
  "github": {
    "silent": false
  },
  "env": {
    "VERCEL": "1"
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false
}